{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc",
    "test": "jest",
    "dev": "cross-env NODE_ENV=development tsc-watch --onSuccess \"node dist/app.js\"",
    "docker:dev": "docker-compose -f docker-compose-dev.yaml --env-file .env.dev up -d --build",
    "docker:dev:down": "docker-compose -f docker-compose-dev.yaml down",
    "docker:dev:rebuild": "docker-compose -f docker-compose-dev.yaml down -v && docker-compose -f docker-compose-dev.yaml up -d --build",
    "docker:prod": "docker-compose -f docker-compose.yaml --env-file .env up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.yaml down",
    "docker:prod:rebuild": "docker-compose -f docker-compose.yaml down -v && docker-compose -f docker-compose.yaml up -d --build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/awilix": "^8.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/postgres": "^6.0.2",
    "@fastify/redis": "^7.0.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@fastify/websocket": "^11.1.0",
    "@sinclair/typebox": "^0.34.37",
    "awilix": "^12.0.5",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.0.0",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "json-schema-to-ts": "^3.1.1",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.0.7",
    "@types/pg": "^8.15.4",
    "@types/ws": "^8.18.1",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.8.3"
  }
